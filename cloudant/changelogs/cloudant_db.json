        }, 
        "by_subOfferId": {
          "map": "function(doc) {if (doc.subOffers) {for (var idx in doc.subOffers) {emit(doc.subOffers[idx], doc._id);}}}"
        }
      }
    }, 
    "price_tier": {
      "views": {
        "by_tierId": {
          "map": "function(doc) {emit(doc._id, doc._id)}"
        }
      }
    }
  }
}

