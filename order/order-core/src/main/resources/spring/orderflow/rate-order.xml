<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xmlns:wf="http://www.junbo.com/schema/webflow"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.junbo.com/schema/webflow http://www.junbo.com/schema/webflow-config/webflow.xsd">

    <wf:flow id="RATE_ORDER">
        <wf:end-state id="endState">
            <wf:on-entry>
                <wf:action ref="validateUserAction"/>
                <wf:action ref="ratingAction"/>
                <wf:action ref="rate_order_createOrderAction"/>
            </wf:on-entry>
        </wf:end-state>
    </wf:flow>

    <bean id="rate_order_createOrderAction" class="com.junbo.order.core.impl.orderaction.SaveOrderAction">
        <property name="orderActionType" value="RATE"></property>
    </bean>

</beans>
