<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/css/bootstrap.min.css' />
    <link rel='stylesheet' href='/css/style.css' />
    <script type="text/javascript" src="scripts/libs/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="scripts/libs/bootstrap.min.js"></script>
    <script type="text/javascript" src="scripts/libs/handlebars-v1.3.0.js"></script>
    <script type="text/javascript" src="scripts/libs/ember.js"></script>
      <script type="text/javascript" src="scripts/libs/ember-data.js"></script>
    <script type="text/javascript" src="scripts/libs/jquery.metadata.js"></script>
    <script type="text/javascript" src="scripts/libs/jquery.validate.min.js"></script>
  </head>
  <body>
  <script type="text/x-handlebars">
      {{outlet}}
  </script>
  <script type="text/x-handlebars" data-template-name="index">
      {{outlet head}}
      <div class="container-fluid">
          <div class="row">
              <div class="col-md-2 sidebar">
                  {{outlet sidebar}}
              </div>
              <div class="col-md-10 col-md-offset-2 main">
                  <div class="row">
                      <div class="col-md-6">{{outlet profiles}}</div>
                      <div class="col-md-6">{{outlet entitlements}}</div>

                      <div class="clearfix"></div>

                      <div class="col-md-6">{{outlet payments}}</div>
                      <div class="col-md-6">{{outlet shipping}}</div>

                      <div class="clearfix"></div>

                      <div class="col-md-6">{{outlet histories}}</div>
                  </div>
              </div>
          </div>
      </div>
  </script>

  <%include common/login%>
  <%include common/head%>
  <%include common/sidebar%>
  <%include common/profiles%>
  <%include common/entitlements%>
  <%include common/payments%>
  <%include common/shipping_info%>
  <%include common/history%>

  <script type="text/javascript" src="scripts/app/common/utils.js"></script>
  <script type="text/javascript" src="scripts/app/common/config.js"></script>
  <script type="text/javascript" src="scripts/app/common/auth_manager.js"></script>
  <script type="text/javascript" src="scripts/app/common/rest_client.js"></script>
  <script type="text/javascript" src="scripts/app/common/data_provider.js"></script>
  <script type="text/javascript" src="scripts/app/models/identity.js"></script>
  <script type="text/javascript" src="scripts/app/app.js"></script>
  </body>
</html>