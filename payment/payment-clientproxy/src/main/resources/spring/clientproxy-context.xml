<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="brainTreeClient" class="com.junbo.payment.spec.internal.proxy.BrainTreeResourceClientProxy"
          parent="baseClientProxy">
        <property name="target" value="${payment.clientproxy.braintree.target}"/>
    </bean>

    <bean id="walletClient" class="com.junbo.ewallet.spec.resource.proxy.WalletResourceClientProxy"
          parent="baseClientProxy">
        <property name="target" value="${clientproxy.ewallet.url}"/>
    </bean>

    <bean id="personalInfoFacade" class="com.junbo.payment.clientproxy.impl.PersonalInfoFacadeImpl"/>
    <bean id="mockPersonalInfoClientProxy" class="com.junbo.payment.clientproxy.MockPersonalInfoClientProxy"/>

</beans>